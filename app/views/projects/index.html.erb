<h1>Project lists</h1>
<table>
  <thead>
    <tr>
      <th>title</th>
      <th>description</th>
    </tr>
  </thead>
  <% @projects.each do |project| %>
    <tbody>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td><%= link_to "Edit", edit_project_path(project) %>
        <%= link_to "Delete", project_path(project),
                                method: :delete,
                                data: {confirm: "Are you sure?"}%></td>
      </tr>
    </tbody>
  <% end %>

</table>
